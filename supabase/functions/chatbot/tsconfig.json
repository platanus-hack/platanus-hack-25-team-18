{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["esnext", "dom"],
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": []
  },
  "include": ["**/*.ts", "deno.d.ts"],
  "exclude": ["node_modules"]
}
